{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "cdk.out/**"]
    },
    "deploy": {
      "dependsOn": ["cdk#build"],
      "outputs": []
    },
    "deploy:prod": {
      "dependsOn": ["cdk#build"],
      "cache": false,
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "synth": {
      "dependsOn": ["cdk#build"],
      "outputs": ["dist/**", "cdk.out/**"]
    }
  }
}
